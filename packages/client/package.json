    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@effect-ak/tg-bot-api": "workspace:*"
  },
  "peerDependencies": {
    "effect": "^3.12.0"
  }
