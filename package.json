{
  "name": "@effect-ak/tg-bot-client",
  "version": "0.2.2",
  "homepage": "https://effect-ak.github.io/telegram-bot-client",
  "author": {
    "name": "Aleksandr Kondaurov",
    "email": "kondaurov.dev@gmail.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "description": "Telegram bot client",
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts"
  ],
  "keywords": [
    "telegram",
    "telegram bot api"
  ],
  "repository": {
    "url": "https://github.com/effect-ak/tg-bot-client"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "docgen": "docgen",
    "gen": "tsx ./codegen/main",
    "echo-bot": "tsx ./example/echo-bot.ts",
    "build": "pnpm build-esm && pnpm build-cjs",
    "build-esm": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "build-cjs": "babel dist/esm --out-dir dist/cjs"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "node-html-parser": "^6.1.13",
    "openapi-types": "^12.1.3",
    "ts-morph": "^24.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8",
    "tsup": "^8.3.5"
  },
  "dependencies": {
    "effect": "^3.4.0"
  }
}
